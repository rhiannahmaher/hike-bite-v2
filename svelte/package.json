		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"leaflet": "^1.9.4",
		"mongoose": "^8.15.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-frappe-charts": "^1.10.0",
